{
  "name": "@prajwolpakka/antd-extended",
  "private": false,
  "version": "1.0.1",
  "description": "Ant Design v6 extended component library",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint .",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage",
    "size": "size-limit",
    "pack": "pnpm pack",
    "prepublishOnly": "pnpm build && pnpm size"
  },
  "peerDependencies": {
    "antd": "6.0.0-alpha.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-jest": "^30.0.5",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "eslint": "^8.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "^3.0.0",
    "size-limit": "^11.2.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "antd": "6.0.0-alpha.1",
      "react": "19.0.0",
      "react-dom": "19.0.0",
      "@types/react": "19.0.7",
      "@types/react-dom": "19.0.3"
    }
  }
}
